<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:esri="http://schemas.esri.com/arcgis/runtime/2013"
        xmlns:local="clr-namespace:WPGIS"  
        x:Class="WPGIS.MainWindow"
        mc:Ignorable="d"
        Title="标绘演示" Icon="/WPGIS;component/logo.png" Height="525" Width="790">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <esri:SceneView x:Name="MySceneView" Margin="0" Grid.Row="1"/>
        <local:UCMapToolbar x:Name="MapToolbar" Grid.Row="0" Margin="0"/>

    </Grid>
</Window>
